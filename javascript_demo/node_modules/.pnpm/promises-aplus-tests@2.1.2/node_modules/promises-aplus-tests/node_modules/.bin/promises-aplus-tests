#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules/promises-aplus-tests/lib/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules/promises-aplus-tests/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules/promises-aplus-tests/lib/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules/promises-aplus-tests/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/promises-aplus-tests@2.1.2/node_modules:/Users/rihime/Desktop/front_basic_demo/javascript_demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
