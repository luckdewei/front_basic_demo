#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules/typescript/bin/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules/typescript/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules/typescript/bin/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules/typescript/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/typescript@5.8.2/node_modules:/Users/rihime/Desktop/front_basic_demo/vue_demo/vue3_lesson/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.8.2/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.8.2/node_modules/typescript/bin/tsc" "$@"
fi
